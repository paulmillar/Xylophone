      <attr name="outerItem"><item/></attr>

      <object rdn="innerItem" list="items-inner">
	<attr name="innerItem"><item/></attr>
      </object>
    </object>
  </publish>

  <lists>
    <list name="items-outer">
      <item>Foo</item>
      <item>Bar</item>
      <item>Baz</item>
    </list>

    <list name="items-inner">
      <item>foo</item>
      <item>bar</item>
      <item>baz</item>
    </list>
  </lists>
</xylophone>
